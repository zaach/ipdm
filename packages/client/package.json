{
  "name": "@ipdm/client",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "lint": "TIMING=1 eslint \"**/*.{ts,js}\" --fix",
    "clean": "rm -rf ./dist && rm -rf .turbo",
    "test": "pnpm test:src && pnpm test:examples",
    "test:src": "tap --no-coverage --node-arg=--loader=tsx src/**/*.test.ts src/*.test.ts",
    "test:examples": "cd examples/browser-to-browser && npm i && npm run test"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@libp2p/interface-connection": "^5.1.0",
    "@types/jest": "^29.2.4",
    "@types/tap": "^15.0.7",
    "@zaach/rollup-plugin-node-polyfills": "^0.2.2",
    "eslint": "^8.11.0",
    "tap": "^16.3.2",
    "tape": "^5.6.1",
    "tsx": "^3.12.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.3.0"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^12.0.0",
    "@chainsafe/libp2p-yamux": "^4.0.1",
    "@libp2p/bootstrap": "^8.0.0",
    "@libp2p/mplex": "^8.0.1",
    "@libp2p/tcp": "^7.0.1",
    "@libp2p/webrtc": "^2.0.2",
    "@libp2p/websockets": "^6.0.1",
    "@multiformats/multiaddr": "^12.0.0",
    "@stablelib/base64": "^1.0.1",
    "@stablelib/ed25519": "^1.0.3",
    "hpke-js": "^0.15.0",
    "it-pipe": "^3.0.1",
    "it-pushable": "^3.1.0",
    "jose": "^4.14.4",
    "libp2p": "^0.45.0",
    "uint8arrays": "^4.0.2"
  }
}
