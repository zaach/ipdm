{
  "name": "@ipdm/client",
  "version": "0.0.1",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "rm -fr dist/* && tsc",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf ./dist && rm -rf .turbo",
    "test": "tap --no-coverage --node-arg=--loader=tsx src/**/*.test.ts"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/tap": "^15.0.7",
    "eslint": "^8.11.0",
    "tap": "^16.3.2",
    "tape": "^5.6.1",
    "tsx": "^3.12.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@stablelib/base64": "^1.0.1",
    "@stablelib/ed25519": "^1.0.3",
    "hpke-js": "^0.15.0",
    "uint8arrays": "^4.0.2"
  }
}
